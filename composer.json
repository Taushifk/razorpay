{
    "name": "taushifk/cashier-razorpay",
    "description": "Cashier razorpay provides an expressive, fluent interface to Paddle's subscription billing services.",
    "keywords": ["laravel", "paddle", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taushif khatri"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/contracts": "^7.9|^8.0",
        "illuminate/database": "^7.9|^8.0",
        "illuminate/http": "^7.9|^8.0",
        "illuminate/routing": "^7.9|^8.0",
        "illuminate/support": "^7.9|^8.0",
        "illuminate/view": "^7.9|^8.0",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "spatie/url": "^1.3.5",
        "symfony/http-kernel": "^5.0",
        "symfony/intl": "^5.0",
        "razorpay/razorpay": "2.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Taushifk\\Razorpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Taushifk\\Razorpay\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
